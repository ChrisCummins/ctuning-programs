{
  "compile_deps": {
    "compiler": {
      "local": "yes",
      "tags": "compiler,lang-cpp"
    }
  },
  "compiler_env": "CK_CXX",
  "main_language": "c",
  "process_in_tmp": "yes",
  "run_cmds": {
    "default": {
      "hot_functions": [
        {
          "name": "matmul",
          "percent": "90"
        }
      ],
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$#BIN_FILE#$ \"$#src_path#$data\"",
        "run_cmd_out1": "tmp-output1.tmp",
        "run_cmd_out2": "tmp-output2.tmp",
        "run_correctness_output_files": [
          "tmp-output.tmp",
          "tmp-output2.tmp"
        ],
        "run_input_files": [
          "data"
        ],
        "run_output_files": [
          "tmp-output.tmp"
        ]
      }
    }
  },
  "run_vars": {
    "CT_MATRIX_DIMENSION": "512",
    "CT_REPEAT_MAIN": "1"
  },
  "run_vars_desc": {
    "##CT_MATRIX_DIMENSION": {
      "default_value": "1",
      "desc_text": "Square matrix dimension",
      "explorable": "yes",
      "explore_level": "1",
      "explore_start": "0",
      "explore_step": "10",
      "explore_stop": "1000000",
      "field_size": "7",
      "forbid_disable_at_random": "yes",
      "property": "yes",
      "skip_form_refresh": "yes",
      "sort_index": "1",
      "type": "integer"
    },
    "##CT_REPEAT_MAIN": {
      "default_value": "1",
      "desc_text": "Main function/kernel repetition",
      "explorable": "yes",
      "explore_level": "1",
      "explore_start": "0",
      "explore_step": "10",
      "explore_stop": "1000000",
      "field_size": "7",
      "forbid_disable_at_random": "yes",
      "property": "yes",
      "skip_form_refresh": "yes",
      "sort_index": "1",
      "type": "integer"
    }
  },
  "source_files": [
    "matmul.cc"
  ],
  "source_files_templates": [
    "matmul.cc"
  ],
  "tags": [
    "demo",
    "program",
    "lang-cpp"
  ],
  "target_file": "a"
}
