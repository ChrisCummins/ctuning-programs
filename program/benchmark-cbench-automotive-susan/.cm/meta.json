{
  "tags": [
    "program", 
    "benchmark",
    "cbench",
    "lang-c",
    "small"
  ], 


  "compile_deps": {
    "compiler": {
      "local": "yes", 
      "tags": "compiler,lang-c"
    }
  }, 

  "source_files": [
    "ctuning-rtl.c", 
    "susan.c"
  ], 

  "compiler_env":"CK_CC",

  "target_file": "a", 

  "compile_individual_files_cmd":{
    "all":"$#COMPILER#$ $#COMPILER_FLAGS_BEFORE#$$#SOURCE_FILE#$ $#COMPILER_OBJ_FLAG#$$#OBJ_FILE#$$#OBJ_EXT#$ $#COMPILER_FLAGS_AFTER#$"
   },

  "link_individual_files_cmd":{
    "all":"$#LINKER#$ $#LINKER_FLAGS_BEFORE#$ $#OBJ_FILES#$ $#LINKER_FLAGS_AFTER#$"
   },

  "run_cmds": {
    "corners": {
      "dataset_tags":["dataset", "image", "pgm"],
      "ignore_return_code":"no",

      "hot_functions": [
        {
          "percent": "95", 
          "name": "susan_corners"
        }
      ], 
      "run_time": {
        "run_cmd_out1": "tmp-output1.tmp", 
        "run_cmd_main": "$#BIN_FILE#$ $#dataset_path#$$#os_dir_separator#$$#dataset_filename#$ tmp-output.tmp -c", 
        "run_cmd_out2": "tmp-output2.tmp", 
        "run_output_files": [
          "tmp-output.tmp"
        ], 
        "run_correctness_output_files": [
          "tmp-output.tmp", 
          "tmp-output2.tmp"
        ]
      }
    }, 
    "edges": {
      "dataset_tags":["dataset", "image", "pgm"],
      "ignore_return_code":"no",
      "hot_functions": [
        {
          "percent": "70", 
          "name": "susan_edges"
        }, 
        {
          "percent": "18", 
          "name": "susan_thin"
        }
      ], 
      "run_time": {
        "run_cmd_out1": "tmp-output1.tmp", 
        "run_cmd_main": "$#BIN_FILE#$ $#dataset_path#$$#os_dir_separator#$$#dataset_filename#$ tmp-output.tmp -e", 
        "run_cmd_out2": "tmp-output2.tmp", 
        "run_output_files": [
          "tmp-output.tmp"
        ], 
        "run_correctness_output_files": [
          "tmp-output.tmp", 
          "tmp-output2.tmp"
        ]
      }
    }, 
    "smoothing": {
      "dataset_tags":["dataset", "image", "pgm"],
      "ignore_return_code":"no",

      "hot_functions": [
        {
          "percent": "98", 
          "name": "susan_smoothing"
        }
      ], 
      "run_time": {
        "run_cmd_out1": "tmp-output1.tmp", 
        "run_cmd_main": "$#BIN_FILE#$ $#dataset_path#$$#os_dir_separator#$$#dataset_filename#$ tmp-output.tmp -s", 
        "run_cmd_out2": "tmp-output2.tmp", 
        "run_output_files": [
          "tmp-output.tmp"
        ], 
        "run_correctness_output_files": [
          "tmp-output.tmp", 
          "tmp-output2.tmp"
        ]
      }
    }
  }, 

  "main_language": "c", 

  "run_vars_desc": {
    "##CT_REPEAT_MAIN": {
      "default_value": "1", 
      "field_size": "7", 
      "skip_form_refresh": "yes", 
      "explore_stop": "1000000", 
      "forbid_disable_at_random": "yes", 
      "sort_index": "1", 
      "explorable": "yes", 
      "explore_level": "1", 
      "desc_text": "Main function/kernel repetition", 
      "explore_start": "0", 
      "explore_step": "10", 
      "property": "yes", 
      "type": "integer"
    }
  }, 

  "run_vars": {
    "CT_REPEAT_MAIN": "1"
  }
}