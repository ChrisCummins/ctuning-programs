{
  "compile_deps": {
    "compiler": {
      "local": "yes", 
      "tags": "compiler,lang-c"
    }
  }, 
  "compiler_env": "CK_CC", 
  "main_language": "c", 
  "process_in_tmp": "yes", 
  "run_cmds": {
    "convert": {
      "dataset_tags": [
        "tiff", 
        "image", 
        "bw", 
        "dataset"
      ], 
      "hot_functions": [
        {
          "name": "LZWDecode", 
          "percent": "43"
        }
      ], 
      "ignore_return_code": "no", 
      "run_time": {
        "run_cmd_main": "$#BIN_FILE#$ $#dataset_path#$$#os_dir_separator#$$#dataset_filename#$ tmp-output.tmp", 
        "run_cmd_out1": "tmp-output1.tmp", 
        "run_cmd_out2": "tmp-output2.tmp", 
        "run_correctness_output_files": [
          "tmp-output.tmp", 
          "tmp-output2.tmp"
        ], 
        "run_output_files": [
          "tmp-output.tmp"
        ]
      }
    }
  }, 
  "run_vars": {
    "CT_REPEAT_MAIN": "1"
  }, 
  "run_vars_desc": {
    "##CT_REPEAT_MAIN": {
      "default_value": "1", 
      "desc_text": "Main function/kernel repetition", 
      "explorable": "yes", 
      "explore_level": "1", 
      "explore_start": "0", 
      "explore_step": "10", 
      "explore_stop": "1000000", 
      "field_size": "7", 
      "forbid_disable_at_random": "yes", 
      "property": "yes", 
      "skip_form_refresh": "yes", 
      "sort_index": "1", 
      "type": "integer"
    }
  }, 
  "source_files": [
    "ctuning-rtl.c", 
    "tiffdither.c", 
    "tif_aux.c", 
    "tif_close.c", 
    "tif_codec.c", 
    "tif_compress.c", 
    "tif_dir.c", 
    "tif_dirinfo.c", 
    "tif_dirread.c", 
    "tif_dirwrite.c", 
    "tif_dumpmode.c", 
    "tif_error.c", 
    "tif_fax3.c", 
    "tif_fax3sm.c", 
    "tif_flush.c", 
    "tif_getimage.c", 
    "tif_jpeg.c", 
    "tif_luv.c", 
    "tif_lzw.c", 
    "tif_next.c", 
    "tif_open.c", 
    "tif_packbits.c", 
    "tif_pixarlog.c", 
    "tif_predict.c", 
    "tif_print.c", 
    "tif_read.c", 
    "tif_strip.c", 
    "tif_swab.c", 
    "tif_thunder.c", 
    "tif_tile.c", 
    "tif_unix.c", 
    "tif_version.c", 
    "tif_warning.c", 
    "tif_write.c", 
    "tif_zip.c", 
    "getopt.c"
  ], 
  "source_files_templates": [
    "ctuning-rtl.c"
  ], 
  "tags": [
    "small", 
    "cbench", 
    "program", 
    "lang-c", 
    "crowd-tuning"
  ], 
  "target_file": "a"
}