{
  "cm_archive": "",
  "cm_file_upload_name": "",
  "cm_file_upload_type": "",
  "compile_deps": {
    "compiler": {
      "local": "yes",
      "tags": "compiler,lang-c"
    }
  },
  "compiler_env": "CK_CC",
  "extra_ld_vars": "$<<CK_EXTRA_LIB_M>>$",
  "main_language": "c",
  "process_in_tmp": "yes",
  "run_cmds": {
    "decode": {
      "dataset_tags": [
        "bzip2",
        "archiver",
        "dataset"
      ],
      "hot_functions": [
        {
          "name": "BZ2_decompress",
          "percent": "55"
        },
        {
          "name": "BZ2_bzDecompress",
          "percent": "44"
        }
      ],
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$#BIN_FILE#$ -d -k -f $#dataset_path#$$#dataset_filename#$",
        "run_cmd_out1": "tmp-output1.tmp",
        "run_cmd_out2": "tmp-output2.tmp",
        "run_correctness_output_files": [
          "bzip2.tmp",
          "tmp-output2.tmp"
        ],
        "run_output_files": [
          "bzip2.tmp"
        ]
      }
    },
    "encode": {
      "dataset_tags": [
        "dataset"
      ],
      "hot_functions": [
        {
          "name": "mainSort",
          "percent": "58"
        },
        {
          "name": "BZ2_compressBlock",
          "percent": "28"
        },
        {
          "name": "BZ2_hbMakeCodeLengths",
          "percent": "6"
        }
      ],
      "ignore_return_code": "no",
      "run_time": {
        "run_cmd_main": "$#BIN_FILE#$ -z -k -f $#dataset_path#$$#dataset_filename#$",
        "run_cmd_out1": "tmp-output1.tmp",
        "run_cmd_out2": "tmp-output2.tmp",
        "run_correctness_output_files": [
          "bzip2.tmp",
          "tmp-output2.tmp"
        ],
        "run_output_files": [
          "bzip2.tmp"
        ]
      }
    }
  },
  "run_vars": {
    "CT_REPEAT_MAIN": "1"
  },
  "run_vars_desc": {
    "##CT_REPEAT_MAIN": {
      "default_value": "1",
      "desc_text": "Main function/kernel repetition",
      "explorable": "yes",
      "explore_level": "1",
      "explore_start": "0",
      "explore_step": "10",
      "explore_stop": "1000000",
      "field_size": "7",
      "forbid_disable_at_random": "yes",
      "property": "yes",
      "skip_form_refresh": "yes",
      "sort_index": "1",
      "type": "integer"
    }
  },
  "source_files": [
    "blocksort.c",
    "bzip2.c",
    "bzlib.c",
    "compress.c",
    "crctable.c",
    "decompress.c",
    "huffman.c",
    "randtable.c",
    "ctuning-rtl.c"
  ],
  "source_files_templates": [
    "ctuning-rtl.c"
  ],
  "tags": [
    "small",
    "cbench",
    "program",
    "lang-c",
    "crowd-tuning"
  ],
  "target_file": "a"
}
